{
    "manifest_version": 2,
    "name": "Add To Lists",
    "description": "Add/bookmark an opened tab URL to multiple lists",
    "version": "1.0",
    "author": "Amit Thakur",
   "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Add To Lists",
      "default_popup": "popup.html"
    },
    "permissions": [
        "storage", "tabs"
    ]

   }